<p>ライブ会場登録</p>

	<%= form_for(@place) do |f| %>
	<p>会場名</p>
	<%= f.text_field :place %>
	<%= f.submit '登録' %>
	<% end %>

	<p>ライブ会場一覧</p>
	<% @places.each do |place| %>
	<%= place.place %>
	<%= link_to "編集", edit_place_path(Place.find_by(id: params[:id])) %>
	<%= link_to "削除", place_path(Place.find_by(id: params[:id])), method: :delete %>
	<% end %>