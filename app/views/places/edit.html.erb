<%= render "partial/flash" %>

<div class="container">
	<div class="row top">
    <div class="col-md-offset-4 col-md-4 col-xs-offset-1 col-xs-10">

			<h4>会場編集</h4>

			<%= form_for(@place) do |f| %>

			<div class="field">
				<p>会場名</p>
				<%= f.text_field :place %>
			</div>

			<div class="actions">
				<%= f.submit '更新' %>
			</div>

			<% end %>

			<%= link_to "会場登録に戻る", new_place_path, class: "btn btn-default" %>

		</div>
	</div>
</div>