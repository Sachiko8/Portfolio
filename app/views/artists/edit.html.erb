<div class="container">
<p>アーティスト編集</p>

<%= form_for(@artist) do |f| %>
  <p>アーティスト名：<%= f.text_field :artist_name %></p>
  <p>アーティスト名フリガナ：<%= f.text_field :artist_kana %></p>
  <p>アーティスト説明：<%= f.text_field :artist_details %></p>
  <p>アーティスト画像：<%= f.attachment_field :thumbnails_images, multiple: true %></p>
  <%= f.submit '更新' %>
<% end %>

<%= link_to "戻る", artist_path(@artist) %>
</div>