<div class="container">
	<p>ライブ編集</p>
<%= @artist.artist_name %>

<%= form_for [@artist, @live] do |f| %>
	<p>日時</p>
	<%= f.date_select :when %>
  <p>ライブ会場</p>
    <%= f.select :place_id, Place.all.collect { |p| [ p.place, p.id ] } %>
  <p>ライブ名</p>
  <%= f.text_field :live_title %>

	<%= f.submit '更新' %>
	<% end %>

	    <%= link_to "アーティスト詳細に戻る", artist_path(@artist.id) %>
	</div>
